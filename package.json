{
  "name": "nodebase",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coveralls": "npm run build && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage && npm run clean",
    "lint": "node_modules/.bin/coffeelint lib/*.coffee",
    "build": "mv lib src && coffee --no-header -o lib -c src",
    "clean": "rm -rf lib && mv src lib",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "release": "npm run lint && npm test && npm publish"
  },
  "author": "Author <author@io.co.za>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^2.1.0",
    "coffee-script": "^1.9.1",
    "coffeelint": "^1.9.2",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.6",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.7.0"
  },
  "bugs": {
    "url": "https://github.com/io-digital/nodebase/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/io-digital/nodebase"
  }
}
